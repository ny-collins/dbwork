
# .env.example

# Application port (use with IPv6 bind address '::' in your server)
PORT=3003

# MongoDB URI (placeholder format, use credentials in real .env)
DB_URI=mongodb://<username>:<password>@localhost:3003/dbname

# Optional: JWT secret for authentication
JWT_SECRET=your_jwt_secret_here

# Optional: Frontend CORS origin
CORS_ORIGIN=http://localhost:5173